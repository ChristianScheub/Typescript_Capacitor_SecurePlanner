{
	"appId": "de.scheub.securePlanner",
	"appName": "Secure-Planner",
	"webDir": "build",
	"server": {
		"androidScheme": "https"
	},
	"plugins": {
		"Filesystem": {
			"androidPermissions": [
				"READ_EXTERNAL_STORAGE",
				"WRITE_EXTERNAL_STORAGE"
			]
		}
	},
	"packageClassList": [
		"AppPlugin",
		"DevicePlugin",
		"FilesystemPlugin",
		"SharePlugin",
		"NativeBiometric"
	]
}
